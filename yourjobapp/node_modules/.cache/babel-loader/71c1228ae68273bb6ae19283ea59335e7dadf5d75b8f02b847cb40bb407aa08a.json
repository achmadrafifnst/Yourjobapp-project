{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HomePage from '../components/HomePage';\nimport HomeServices from '@/services/home-service';\nexport default {\n  name: 'HomeView',\n  mounted() {\n    if (this.name != '') {\n      this.afterLogin();\n    }\n    // this.afterLogin();\n    this.ResponsiveAppbar();\n    window.addEventListener('resize', this.ResponsiveAppbar);\n  },\n  components: {\n    HomePage\n  },\n  data() {\n    return {\n      name: '',\n      isMobileView: false,\n      items: [{\n        title: 'Home',\n        href: '',\n        to: {\n          name: 'home'\n        },\n        click: this.Home\n      }, {\n        title: 'Contact Me',\n        href: 'https://achmadrafifnasution.vercel.app/contact',\n        to: '',\n        click: this.Home\n      }, {\n        title: 'Logout',\n        href: '',\n        to: '',\n        click: this.logOut\n      }]\n    };\n  },\n  methods: {\n    Home() {\n      console.log('anda berada di home page');\n    },\n    ResponsiveAppbar() {\n      const isMobile = window.matchMedia('(max-width: 959px)').matches;\n      this.isMobileView = isMobile;\n    },\n    logOut() {\n      this.$store.dispatch('auth/logout');\n      this.$router.push('/');\n    },\n    afterLogin() {\n      HomeServices.getHome().then(response => {\n        console.log(response.data.msg);\n        this.name = response.data.name;\n      }, error => {\n        if (error.response.status == 401) {\n          this.$store.dispatch('auth/logout');\n          this.$router.push('/login');\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["HomePage","HomeServices","name","mounted","afterLogin","ResponsiveAppbar","window","addEventListener","components","data","isMobileView","items","title","href","to","click","Home","logOut","methods","console","log","isMobile","matchMedia","matches","$store","dispatch","$router","push","getHome","then","response","msg","error","status"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div style=\"background-color: #f3e6da;\">\n    <v-app-bar\n      app\n      color=\"#64320C\"\n      dark\n      fixed\n      class=\"appBar\"\n    >\n\n      <h3> Hai, {{ this.name }}</h3>\n\n      <v-spacer></v-spacer>\n\n      <v-btn\n        text\n        class=\"hidden-sm-and-down\"\n        :to=\"{name:'home'}\"\n      >\n        <span class=\"ma-0\">Home</span>\n      </v-btn>\n      \n      <v-btn\n        href=\"https://achmadrafifnasution.vercel.app/contact\"\n        text\n        class=\"hidden-sm-and-down\"\n      >\n        <span class=\"ma-0\">Contact Me</span>\n      </v-btn>\n\n      <v-btn icon class=\"hidden-sm-and-down\"  @click.prevent=\"logOut\">\n        <v-icon>mdi-logout</v-icon>\n      </v-btn>\n\n      <v-menu offset-y v-if=\"isMobileView\">\n        <template v-slot:activator=\"{ on, attrs }\">\n          <v-btn\n            color=\"white\"\n            dark\n            v-bind=\"attrs\"\n            v-on=\"on\"\n            text\n          >\n            <v-icon>mdi-menu</v-icon>\n          </v-btn>\n        </template>\n        <v-list>\n          <v-list-item\n            v-for=\"(item, index) in items\"\n            :key=\"index\"\n            :href=\"item.href\"\n            :to=\"item.to\"\n            @click=\"item.click\"\n          >\n            <v-list-item-title>{{ item.title }}</v-list-item-title>\n          </v-list-item>\n        </v-list>\n      </v-menu>\n\n    </v-app-bar>\n    <v-main>\n      <HomePage :name=\"name\"/>\n      <v-row style=\"min-height: 5px; background-color: black;\"></v-row>\n      <v-row style=\"min-height: 50px; background-color: white;\">\n        <v-col cols=\"12\" md=\"12\" class=\"d-flex align-center justify-center\" id=\"footer\">\n          Developed By <a href=\"https://achmadrafifnasution.vercel.app\" class=\"link\"> Achmad Rafif Nasution</a>\n        </v-col>\n      </v-row>\n    </v-main>\n  </div>\n</template>\n\n<script>\n  import HomePage from '../components/HomePage'\n  import HomeServices from '@/services/home-service'\n\n  export default {\n    name: 'HomeView',\n    mounted(){\n      if(this.name!=''){\n        this.afterLogin();\n      }\n      // this.afterLogin();\n      this.ResponsiveAppbar();\n      window.addEventListener('resize', this.ResponsiveAppbar);\n    },\n    components: {\n      HomePage,\n    },\n    data(){\n      return{\n        name:'',\n        isMobileView :false,\n        items: [\n          { title: 'Home',href:'',to:{name:'home'},click:this.Home },\n          { title: 'Contact Me',href:'https://achmadrafifnasution.vercel.app/contact',to:'',click:this.Home },\n          { title: 'Logout',href:'',to:'',click:this.logOut}\n        ],\n      }\n    },\n    methods:{\n      Home(){\n        console.log('anda berada di home page')\n      },\n      ResponsiveAppbar(){\n        const isMobile = window.matchMedia('(max-width: 959px)').matches;\n        this.isMobileView = isMobile;\n      },\n      logOut(){\n        this.$store.dispatch('auth/logout');\n        this.$router.push('/');\n      },\n      afterLogin(){\n        HomeServices.getHome().then(\n          response => {\n            console.log(response.data.msg)\n            this.name = response.data.name\n          },\n          error => {\n              if(error.response.status==401){\n                this.$store.dispatch('auth/logout');\n                this.$router.push('/login');\n              }\n          }\n        )\n      }\n    }\n  }\n</script>\n<style scoped>\n/* .appBar{\n        padding-right: 40px;\n    } */\n#footer{\n  font-family: 'Times New Roman';\n}\n\n#footer .link{\n  color: black;\n  margin-left:5px;\n}\n\n.appBar h3{\n  margin-right:20px;\n}\n</style>"],"mappings":";AAyEA,OAAAA,QAAA;AACA,OAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,QAAA;IACA,SAAAD,IAAA;MACA,KAAAE,UAAA;IACA;IACA;IACA,KAAAC,gBAAA;IACAC,MAAA,CAAAC,gBAAA,gBAAAF,gBAAA;EACA;EACAG,UAAA;IACAR;EACA;EACAS,KAAA;IACA;MACAP,IAAA;MACAQ,YAAA;MACAC,KAAA,GACA;QAAAC,KA